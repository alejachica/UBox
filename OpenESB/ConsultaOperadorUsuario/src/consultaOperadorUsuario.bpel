<?xml version="1.0" encoding="UTF-8" ?>
<process
    name="consultaOperadorUsuario"
    targetNamespace="http://enterprise.netbeans.org/bpel/ConsultaOperadorUsuario/consultaOperadorUsuario"
    xmlns:tns="http://enterprise.netbeans.org/bpel/ConsultaOperadorUsuario/consultaOperadorUsuario"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2"
    xmlns:sxat="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Attachment"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling">
    <import namespace="http://j2ee.netbeans.org/wsdl/ConsultaOperadorUsuario/UserProcess/newWSDL" location="UserProcess/consultaOperadorWSDL.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/UsuariosEJBWrapper" location="UsuariosEJBWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://ejb.umbrella.uniandes.edu.co/" location="http://arnaor:8080/UsuariosEJBService/UsuariosEJB?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="ConsultaCentralizador" xmlns:tns="http://enterprise.netbeans.org/bpel/UsuariosEJBWrapper" partnerLinkType="tns:UsuariosEJBLinkType" partnerRole="UsuariosEJBRole"/>
        <partnerLink name="ConsultaOpESB" xmlns:tns="http://j2ee.netbeans.org/wsdl/ConsultaOperadorUsuario/UserProcess/newWSDL" partnerLinkType="tns:newWSDL" myRole="newWSDLPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="NewWSDLOperationOut" xmlns:tns="http://j2ee.netbeans.org/wsdl/ConsultaOperadorUsuario/UserProcess/newWSDL" messageType="tns:newWSDLOperationResponse"/>
        <variable name="ConsultarOperadorUsuarioOut" xmlns:tns="http://ejb.umbrella.uniandes.edu.co/" messageType="tns:consultarOperadorUsuarioResponse"/>
        <variable name="ConsultarOperadorUsuarioIn" xmlns:tns="http://ejb.umbrella.uniandes.edu.co/" messageType="tns:consultarOperadorUsuario"/>
        <variable name="NewWSDLOperationIn" xmlns:tns="http://j2ee.netbeans.org/wsdl/ConsultaOperadorUsuario/UserProcess/newWSDL" messageType="tns:newWSDLOperationRequest"/>
    </variables>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="ConsultaOpESB" operation="newWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/ConsultaOperadorUsuario/UserProcess/newWSDL" portType="tns:newWSDLPortType" variable="NewWSDLOperationIn"/>
        <assign name="Assign1">
            <copy>
                <from variable="NewWSDLOperationIn" part="tipoDocumento"/>
                <to>$ConsultarOperadorUsuarioIn.parameters/arg0</to>
            </copy>
            <copy>
                <from variable="NewWSDLOperationIn" part="numeroDocumento"/>
                <to>$ConsultarOperadorUsuarioIn.parameters/arg1</to>
            </copy>
        </assign>
        <invoke name="Invoke1" partnerLink="ConsultaCentralizador" operation="consultarOperadorUsuario" xmlns:tns="http://ejb.umbrella.uniandes.edu.co/" portType="tns:UsuariosEJB" inputVariable="ConsultarOperadorUsuarioIn" outputVariable="ConsultarOperadorUsuarioOut"/>
        <assign name="Assign2">
            <copy>
                <from>$ConsultarOperadorUsuarioOut.parameters/return</from>
                <to variable="NewWSDLOperationOut" part="part1"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="ConsultaOpESB" operation="newWSDLOperation" xmlns:tns="http://j2ee.netbeans.org/wsdl/ConsultaOperadorUsuario/UserProcess/newWSDL" portType="tns:newWSDLPortType" variable="NewWSDLOperationOut"/>
    </sequence>
</process>
